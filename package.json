{
  "name": "ui-microservices",
  "version": "0.0.0",
  "description": "ui-microservices example",
  "main": "index.js",
  "scripts": {
    "start": "run-p start:*",
    "start:root": "cd ./root && npm start",
    "start:react-service": "cd ./react-service && npm run watch",
    "start:angular-service": "cd ./angular-service && npm start",
    "start:vue-service": "cd ./vue-service && npm run serve",
    "installAll": "npm i && run-p installAll:*",
    "installAll:root": "cd ./root && npm install",
    "installAll:react-service": "cd ./react-service && npm install",
    "installAll:angular-service": "cd ./angular-service && npm install",
    "installAll:vue-service": "cd ./vue-service && npm install",
    "build": "run-p build:*",
    "build:root": "cd ./root && npm start",
    "build:react-service": "cd ./react-service && npm run build",
    "build:angular-service": "cd ./angular-service && npm run build",
    "build:vue-service": "cd ./vue-service && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BezzubovEgor/ui-microservices.git"
  },
  "keywords": [
    "microservices",
    "ui",
    "react",
    "frontend"
  ],
  "author": "\u0016BezzubovEgor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BezzubovEgor/ui-microservices/issues"
  },
  "homepage": "https://github.com/BezzubovEgor/ui-microservices#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
